{
  "name": "express-rest-api-boilerplate",
  "version": "0.0.0",
  "author": "Lukas Aichbauer <l.aichbauer@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aichbauer/express-rest-api-boilerplate.git"
  },
  "homepage": "https://github.com/aichbauer/express-rest-api-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/aichbauer/express-rest-api-boilerplate/issues"
  },
  "scripts": {
    "start": "npm install && node app.js",
    "poststart": "npm run create-sqlite-db",
    "dev": "cross-env NODE_ENV=development node app.js",
    "nodemon": "nodemon --exec npm run dev",
    "production": "cross-env NODE_ENV=production node app.js",
    "test": "npm runcross-env NODE_ENV=testing jest --coverage;",
    "test-ci": "jest --coverage --forceExit"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.2",
    "core-js": "^2.6.9",
    "cors": "^2.8.4",
    "cross-fetch": "^3.0.6",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-routes-mapper": "^1.0.2",
    "helmet": "^3.12.0",
    "jsonwebtoken": "^8.2.1"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.11.0",
    "husky": "^0.14.3",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "shx": "^0.2.2",
    "supertest": "^3.0.0"
  }
}
